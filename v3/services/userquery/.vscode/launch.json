    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch test function",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}",
            "env": {
                "GO111MODULE": "on"
            },
            "args": [
                "-test.run",
                "MyTestFunction"
            ],
            "showLog": true
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "GO111MODULE": "on"
            },
            "args": [],
            "showLog": true
        }
    ]
}